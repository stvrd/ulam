create.ulam2 <- function(vec) {
n <- sqrt(length(vec))
x <- ceiling(n/2+0.25)
y <- ceiling(n/2)
ulam <- matrix(nrow = n, ncol=n)
dir <- c("r","u","l","d")
n.rep <- rep(1:n,each=2)
dirs <- dir %>% rep(length.out=length(n.rep)) %>% rep(n.rep)
ulam[x,y] <- -1
for(i in 1:n^2) {
if (dirs[i]=="r") { y <- y+1
}  else if (dirs[i]=="u") { x <- x-1
}  else if (dirs[i]=="l") { y <- y-1
}  else if (dirs[i]=="d") { x <- x+1
}  else stop()
ulam[x,y] <- vec[i+1]
ulam
}
ulam
}
